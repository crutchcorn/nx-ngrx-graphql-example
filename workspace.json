{
  "version": 1,
  "projects": {
    "apollo-express-example": {
      "root": "apps/apollo-express-example",
      "sourceRoot": "apps/apollo-express-example/src",
      "projectType": "application",
      "prefix": "apollo-express-example",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/apollo-express-example",
            "main": "apps/apollo-express-example/src/main.ts",
            "tsConfig": "apps/apollo-express-example/tsconfig.app.json",
            "assets": ["apps/apollo-express-example/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/apollo-express-example/src/environments/environment.ts",
                  "with": "apps/apollo-express-example/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "apollo-express-example:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/apollo-express-example/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/apollo-express-example/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ngrx-apollo-example": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/ngrx-apollo-example",
      "sourceRoot": "apps/ngrx-apollo-example/src",
      "prefix": "nx-ngrx-graphql-example",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/ngrx-apollo-example",
            "index": "apps/ngrx-apollo-example/src/index.html",
            "main": "apps/ngrx-apollo-example/src/main.ts",
            "polyfills": "apps/ngrx-apollo-example/src/polyfills.ts",
            "tsConfig": "apps/ngrx-apollo-example/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/ngrx-apollo-example/src/favicon.ico",
              "apps/ngrx-apollo-example/src/assets"
            ],
            "styles": ["apps/ngrx-apollo-example/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ngrx-apollo-example/src/environments/environment.ts",
                  "with": "apps/ngrx-apollo-example/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ngrx-apollo-example:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ngrx-apollo-example:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ngrx-apollo-example:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/ngrx-apollo-example/tsconfig.app.json",
              "apps/ngrx-apollo-example/tsconfig.spec.json",
              "apps/ngrx-apollo-example/tsconfig.editor.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/ngrx-apollo-example/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/ngrx-apollo-example/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ngrx-apollo-example-e2e": {
      "root": "apps/ngrx-apollo-example-e2e",
      "sourceRoot": "apps/ngrx-apollo-example-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ngrx-apollo-example-e2e/cypress.json",
            "tsConfig": "apps/ngrx-apollo-example-e2e/tsconfig.e2e.json",
            "devServerTarget": "ngrx-apollo-example:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ngrx-apollo-example:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/ngrx-apollo-example-e2e/tsconfig.e2e.json"],
            "exclude": [
              "**/node_modules/**",
              "!apps/ngrx-apollo-example-e2e/**/*"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/express"
  },
  "defaultProject": "apollo-express-example",
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  }
}
